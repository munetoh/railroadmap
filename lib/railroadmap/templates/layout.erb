  <body>
    <div id="loading">
      <img src="<%= assets_path('loading.gif') %>" alt="loading"/>
    </div>
    <div id="wrapper" style="display:none;">
      <div class="timestamp">Generated <%= timeago(Time.now) %></div>
      <ul class="group_tabs"></ul>

      <div id="content">
        
        <%= warning("Security warnings", smodel) %>
        
        <%= design("Security design", smodel) %>
 
        <%= navmodel("Navigation model", smodel) %>       
        
      </div>
    
      <div id="footer">
        Generated by <a href="http://github.com/munetoh/railroadmap">railroadmap</a> v<%= Railroadmap::VERSION %> 
        <br/>
        using <%= smodel.command_name %>
      </div>
    
      <div class="source_files">
      <% smodel.source_files.each do |source_file| %>
        <%= formatted_source_file(source_file) %>
      <% end %>
      </div>
    </div>
  </body>
